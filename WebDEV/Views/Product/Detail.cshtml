@model WebDEV.Areas.admincp.Models.product
@{

    Layout = "~/Views/Shared/_RealPlaceDetails.cshtml";
}
<!-- .site-header -->
<div class="page-head " style="background: url(../real-places/images/banner.jpg) #494c53 no-repeat center top;  background-size: cover;">
    <div class="breadcrumb-wrapper">
        <div class="container">
            <nav>
                <ol class="breadcrumb">
                    <li>
                        <a href="/" title="Trang chủ">Trang chủ</a>
                    </li>
                    <li>
                        <a href="~/@Model.product_category.url" title="@Model.product_category.name">@Model.product_category.name</a>
                    </li>
                    <li class="active">   <a href="~/@Model.url" title="@Model.name">@Model.name</a> </li>
                </ol>
            </nav>
        </div>
    </div>
    <!-- .breadcrumb-wrapper -->
</div>
<!-- .page-head -->
<div id="content-wrapper" class="site-content-wrapper site-pages">
    <div id="content" class="site-content layout-boxed">
        <div class="container">
            <div class="container-property-single clearfix">
                <div class="col-md-8 site-main-content property-single-content">
                    <main id="main" class="site-main">
                        <article class="hentry clearfix">
                            <div class="single-property-slider">
                                <ul id="image-gallery" class="list-unstyled">
                                    <li data-thumb="../Upload/images/products/@Model.images">
                                        <a class="swipebox" data-rel="gallery12" href="../Upload/images/products/@Model.images"><img src="~/Upload/images/products/@Model.images" alt="@Model.name"></a>
                                    </li>
                                    @Html.Raw(ViewBag.ListImage)

                                </ul>
                            </div>
                            <div id="property-featured-image" class="only-for-print">
                                <img src="~/Upload/images/products/@Model.images" alt="@Model.name">
                            </div>

                            <div class="single-property-wrapper">
                                <header class="entry-header single-property-header">
                                    <h1 class="entry-title single-property-title">@Model.name</h1>
                                    <span class="single-property-price price">Giá: @Model.price</span>
                                </header>
                                <div class="property-meta entry-meta clearfix ">

                                    <div class="meta-item">
                                        <i class="meta-item-icon icon-area">
                                            <svg xmlns="http://www.w3.org/2000/svg" class="meta-icon-container" width="30" height="30" viewBox="0 0 48 48">
                                                <path class="meta-icon" fill="#660000" d="M46 16v-12c0-1.104-.896-2.001-2-2.001h-12c0-1.103-.896-1.999-2.002-1.999h-11.997c-1.105 0-2.001.896-2.001 1.999h-12c-1.104 0-2 .897-2 2.001v12c-1.104 0-2 .896-2 2v11.999c0 1.104.896 2 2 2v12.001c0 1.104.896 2 2 2h12c0 1.104.896 2 2.001 2h11.997c1.106 0 2.002-.896 2.002-2h12c1.104 0 2-.896 2-2v-12.001c1.104 0 2-.896 2-2v-11.999c0-1.104-.896-2-2-2zm-4.002 23.998c0 1.105-.895 2.002-2 2.002h-31.998c-1.105 0-2-.896-2-2.002v-31.999c0-1.104.895-1.999 2-1.999h31.998c1.105 0 2 .895 2 1.999v31.999zm-5.623-28.908c-.123-.051-.256-.078-.387-.078h-11.39c-.563 0-1.019.453-1.019 1.016 0 .562.456 1.017 1.019 1.017h8.935l-20.5 20.473v-8.926c0-.562-.455-1.017-1.018-1.017-.564 0-1.02.455-1.02 1.017v11.381c0 .562.455 1.016 1.02 1.016h11.39c.562 0 1.017-.454 1.017-1.016 0-.563-.455-1.019-1.017-1.019h-8.933l20.499-20.471v8.924c0 .563.452 1.018 1.018 1.018.561 0 1.016-.455 1.016-1.018v-11.379c0-.132-.025-.264-.076-.387-.107-.249-.304-.448-.554-.551z"></path>
                                            </svg>
                                        </i>
                                        <div class="meta-inner-wrapper">
                                            <span class="meta-item-label">Diện tích: </span>
                                            <span class="meta-item-value">@Model.content2<sub class="meta-item-unit">Sq Ft</sub></span>
                                        </div>
                                    </div>
                                    <div class="meta-item">
                                        <i class="meta-item-icon icon-tag">
                                            <svg xmlns="http://www.w3.org/2000/svg" class="meta-icon-container" width="30" height="30" viewBox="0 0 48 48">
                                                <path class="meta-icon" fill-rule="evenodd" clip-rule="evenodd" fill="#660000" d="M47.199 24.176l-23.552-23.392c-.504-.502-1.174-.778-1.897-.778l-19.087.09c-.236.003-.469.038-.696.1l-.251.1-.166.069c-.319.152-.564.321-.766.529-.497.502-.781 1.196-.778 1.907l.092 19.124c.003.711.283 1.385.795 1.901l23.549 23.389c.221.218.482.393.779.523l.224.092c.26.092.519.145.78.155l.121.009h.012c.239-.003.476-.037.693-.098l.195-.076.2-.084c.315-.145.573-.319.791-.539l18.976-19.214c.507-.511.785-1.188.781-1.908-.003-.72-.287-1.394-.795-1.899zm-35.198-9.17c-1.657 0-3-1.345-3-3 0-1.657 1.343-3 3-3 1.656 0 2.999 1.343 2.999 3 0 1.656-1.343 3-2.999 3z"></path>
                                            </svg>
                                        </i>
                                        <div class="meta-inner-wrapper">
                                            <span class="meta-item-label">Loại</span>
                                            <span class="meta-item-value">@Model.product_category.name</span>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="entry-content clearfix">
                                <h4 class="fancy-title">Mô tả</h4>
                                <div class="property-content">
                                    <p>@Model.desciption</p>
                                </div>
                                <div class="property-additional-details clearfix">
                                    @Html.Raw(Model.content1)
                                </div>
                            </div>
                        </article>
                        <section class="property-location-section clearfix">
                            <h4 class="fancy-title">Bản đồ chỉ đường</h4>
                            <div id="property-map">
                                @Html.Raw(Model.content4)
                            </div>


                        </section>


                    </main>
                    <!-- .site-main -->
                </div>
                <!-- .site-main-content -->
                <div class="col-md-4 zero-horizontal-padding">
                    <aside class="sidebar sidebar-property-detail">
                        <section class="agent-sidebar-widget clearfix">
                            <div class="agent-content-wrapper agent-common-styles">
                                <ul class="agent-contacts-list">
                                    <li class="office">
                                        <svg xmlns="http://www.w3.org/2000/svg" class="contacts-icon-container" width="24" height="24" viewBox="0 0 24 24">
                                            <path class="contacts-icon" fill-rule="evenodd" clip-rule="evenodd" fill="#0080BC" d="M1.027 4.846l-.018.37.01.181c.068 9.565 7.003 17.42 15.919 18.48.338.075 1.253.129 1.614.129.359 0 .744-.021 1.313-.318.328-.172.448-.688.308-1.016-.227-.528-.531-.578-.87-.625-.435-.061-.905 0-1.521 0-1.859 0-3.486-.835-4.386-1.192l.002.003-.076-.034c-.387-.156-.696-.304-.924-.422-3.702-1.765-6.653-4.943-8.186-8.896-.258-.568-1.13-2.731-1.152-6.009h.003l-.022-.223c0-1.727 1.343-3.128 2.999-3.128 1.658 0 3.001 1.401 3.001 3.128 0 1.56-1.096 2.841-2.526 3.079l.001.014c-.513.046-.914.488-.914 1.033 0 .281.251 1.028.251 1.028.015 0 .131.188.119.188-.194-.539 1.669 5.201 7.021 7.849-.001.011.636.309.636.309.47.3 1.083.145 1.37-.347.09-.151.133-.32.14-.488.356-1.306 1.495-2.271 2.863-2.271 1.652 0 2.991 1.398 2.991 3.12 0 .346-.066.671-.164.981-.3.594-.412 1.21.077 1.699.769.769 1.442-.144 1.442-.144.408-.755.643-1.625.643-2.554 0-2.884-2.24-5.222-5.007-5.222-1.947 0-3.633 1.164-4.46 2.858-2.536-1.342-4.556-3.59-5.656-6.344 1.848-.769 3.154-2.647 3.154-4.849 0-2.884-2.241-5.222-5.007-5.222-2.41 0-4.422 1.777-4.897 4.144l-.091.711z" />
                                        </svg>
                                        <span>Office:</span>09678.654.04
                                    </li>
                                    <li class="mobile">
                                        <svg xmlns="http://www.w3.org/2000/svg" class="contacts-icon-container" width="24" height="24" viewBox="0 0 24 24">
                                            <path class="contacts-icon" fill-rule="evenodd" clip-rule="evenodd" fill="#660000" d="M17.001 23.999h-10.001c-1.657 0-3-1.343-3-2.999v-18c0-1.656 1.343-3 3-3h10.001c1.655 0 2.999 1.344 2.999 3v18c0 1.656-1.344 2.999-2.999 2.999zm.999-19.999c0-1.105-.896-2-2-2h-8c-1.105 0-2 .896-2 2v16c0 1.104.895 2 2 2h8c1.104 0 2-.896 2-2v-16zm-4 1h-4c-.552 0-1-.447-1-1 0-.552.448-1 1-1h4c.553 0 1.001.448 1.001 1 0 .553-.448 1-1.001 1zm-2 13c1.105 0 2 .672 2 1.499 0 .829-.895 1.501-2 1.501s-2-.672-2-1.501c0-.827.895-1.499 2-1.499z" />
                                        </svg>
                                        <span>Mobile:</span>0964.900.534
                                    </li>

                                    <li class="map-pin">
                                        <svg xmlns="http://www.w3.org/2000/svg" class="contacts-icon-container" width="40" height="40" viewBox="0 0 24 24">
                                            <path class="contacts-icon" fill-rule="evenodd" clip-rule="evenodd" fill="#000000" d="M12 23.999c.048 0-8-11.582-8-15.999 0-4.419 3.581-8 8-8s8 3.581 8 8c0 4.417-8.084 15.999-8 15.999zm0-19.001c-1.658 0-3.002 1.345-3.002 3.002 0 1.657 1.344 3.002 3.002 3.002 1.657 0 3.002-1.345 3.002-3.002 0-1.658-1.345-3.002-3.002-3.002z" />
                                        </svg>
                                        Quận 6, TPHCM
                                    </li>
                                </ul>
                                <p>Mua bán bất động sản uy tín nhất...</p>
                                <a class="btn-default show-details" href="tel:">Gọi Ngay<i class="fa fa-angle-right"></i></a>

                            </div>
                        </section>
                        <section class="similar-properties meta-item-half clearfix">
                            <div class="nav-and-title clearfix">
                                <h3 class="title">Tin khác</h3>
                                <div class="similar-properties-carousel-nav carousel-nav">
                                    <a class="carousel-prev-item prev">
                                        <svg xmlns="http://www.w3.org/2000/svg" class="arrow-container" width="32" height="52" viewBox="0 0 32 52">
                                            <g class="left-arrow" fill="#fff">
                                                <path opacity=".5" d="M31.611 7.646l-6.787-7.057-24.435 25.406 6.787 7.057z" />
                                                <path d="M.389 26.006l6.787-7.058 24.435 25.406-6.787 7.057z" />
                                            </g>
                                        </svg>
                                    </a>
                                    <a class="carousel-next-item next">
                                        <svg xmlns="http://www.w3.org/2000/svg" class="arrow-container" width="32" height="52" viewBox="0 0 32 52">
                                            <g class="right-arrow" fill-rule="evenodd" clip-rule="evenodd" fill="#fff">
                                                <path d="M.388 44.354l6.788 7.057 24.436-25.406-6.788-7.057-24.436 25.406z" />
                                                <path opacity=".5" d="M31.612 25.994l-6.788 7.058-24.436-25.406 6.788-7.057 24.436 25.405z" />
                                            </g>
                                        </svg>
                                    </a>
                                </div>
                            </div>
                            <div class="similar-properties-carousel">
                                <div class="owl-carousel">
                                    @Html.Raw(ViewBag.ProductCategory)
                                </div>
                            </div>
                        </section>

                    </aside>
                    <!-- .sidebar -->
                </div>
                <!-- .site-sidebar-content -->
            </div>
            <!-- .container-property-single -->
        </div>
        <!-- .container -->
    </div>
    <!-- .site-content -->
</div>
<!-- .site-content-wrapper -->
