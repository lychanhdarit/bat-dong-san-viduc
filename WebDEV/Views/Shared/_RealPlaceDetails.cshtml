<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title </title>
    @Html.Partial("_header_page")
    @Styles.Render("~/Content/real-places")
    <style>
        .meta-wrapper {
            padding: 10px;
            background: #fff;
            border-top: 1px solid #f3f3f3;
            font-size: 0.9em;
            color: #660000;
            padding-left: 20px;
        }

        .single-property-price {
            font-size: 20px;
            padding-top: 19px;
            top: 9px;
            line-height: 2em;
        }

        .gallery-title-wrapper {
            padding: 15px 20px 0px 20px;
        }

        .header-variation-two .main-menu > li:hover > a, .header-variation-two .main-menu > .current-menu-item > a {
            background-color: #660000;
        }
    </style>
</head>
<body>
    <div class="page-loader">
        <img class="page-loader-img" src="~/real-places/images/page-loader-img.gif" alt="Page Loader" />
    </div>
    @Html.Partial("_header_menu")
    @RenderBody()
    <div style="background:#971718">
        <div class="container">
            <h2 style="margin: 0; padding: 15px;  font-size: 16px; color: #fff;padding-left:0;">ĐỊA ỐC PHỒN VINH</h2>
        </div>
    </div>
    @Html.Partial("_footer")
    <!-- .footer -->
    @Scripts.RenderFormat("<script src='{0}' defer async></script>", "~/bundles/real-places")
</body>
</html>
